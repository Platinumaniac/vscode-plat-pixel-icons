{
  "name": "platpixelicons",
  "displayName": "plat Pixel Icons",
  "description": "pixel art icons for vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "Platinumaniac",
  "icon": "./wiz_new_crop.png",
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "iconThemes": [
      {
        "id": "platpixelicons",
        "label": "Plat pixel icons",
        "path": "./icon_themes/platpixel.json"
      }
    ]
  },
  "repository": "https://github.com/Platinumaniac/vscode-plat-pixel-icons",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
